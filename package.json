{
  "name": "vscode-utils",
  "displayName": "utils",
  "description": "vscode utils that are not supported by default.",
  "version": "0.0.1",
  "publisher": "danztran",
  "repository": {
    "type": "git",
    "url": "git://github.com/danztran/vscode-utils.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-utils.removeOtherFoldersFromWorkspace"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-utils.removeOtherFoldersFromWorkspace",
        "title": "Utils: Remove Other Folders from Workspace"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm esbuild-base --minify",
    "vscode:install": "pnpm compile && pnpm vsce package -o out.vsix && code --install-extension out.vsix",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test-compile": "tsc -p ./",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "pnpm esbuild-base --sourcemap",
    "esbuild-watch": "pnpm esbuild-base --sourcemap --watch"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "esbuild": "^0.13.3",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "vsce": "^1.100.1"
  },
  "dependencies": {},
  "packageManager": "pnpm@8.12.1+sha1.aa961ffce9b6eaa56307d9b5ff7e984f25b7eb58"
}
